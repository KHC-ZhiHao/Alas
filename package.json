{
  "name": "alas",
  "version": "0.4.9",
  "description": "View Model System.",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc mocha --require ts-node/register ./test/*.ts",
    "prod": "npm run test && npm run build",
    "build": "cross-env webpack --progress",
    "coverage": "nyc --reporter=lcov npm test",
    "docs": "vuepress dev docs-code",
    "build-docs": "vuepress build docs-code"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KHC-ZhiHao/Alas.git"
  },
  "author": "khczhihao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KHC-ZhiHao/Alas/issues"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.js",
      "**/*.d.ts",
      "node_modules",
      "example/**/*.ts",
      "test/**/*.ts"
    ],
    "all": true
  },
  "homepage": "https://github.com/KHC-ZhiHao/Alas#readme",
  "devDependencies": {
    "coveralls": "^3.0.6",
    "vuepress": "^1.9.7",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/chai": "^4.2.22",
    "@types/jsdom": "^16.2.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^13.13.45",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-webpack-plugin": "^3.2.0",
    "jsdom": "^18.1.1",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
